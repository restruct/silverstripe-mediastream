{
  "name": "restruct/silverstripe-mediastream",
  "description": "Add a timeline of combined cross-platform updates to your website",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "keywords": [
    "silverstripe",
    "social",
    "twitter",
    "facebook"
  ],
  "authors": [
    {
      "name": "restruct"
    }
  ],
  "require": {
    "abraham/twitteroauth": "*",
    "facebook/graph-sdk": "dev-master",
    "php-http/guzzle6-adapter": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Restruct\\Silverstripe\\MediaStream\\": "src/",
      "Restruct\\Silverstripe\\MediaStream\\Tasks\\": "src/Feed/Tasks"
    },
    "classmap": [
      "src"
    ]
  },
  "extra": {
    "installer-name": "mediastream"
  }
}
